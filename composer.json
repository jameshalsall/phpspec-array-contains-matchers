{
    "name": "jameshalsall/phpspec-array-contains-matchers",
    "description": "Provides additional contains matchers for PHPSpec",
    "type": "phpspec-extension",
    "require": {
        "php": ">=7.1",
        "phpspec/phpspec": "^4.0 || ^5.0"
    },
    "require-dev": {
        "behat/behat": "^3.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "James Halsall",
            "email": "james.t.halsall@googlemail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "JamesHalsall\\PhpSpecContainsMatchers\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
